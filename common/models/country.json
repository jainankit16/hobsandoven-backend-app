{
  "name": "Country",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "length": 50
    },
    "capital": {
      "type": "string",
      "length": 30
    },
    "area": {
      "type": "number"
    },
    "population": {
      "type": "number"
    },
    "currency_code": {
      "type": "string",
      "length": 3,
      "dataType": "CHAR"
    },
    "currency_symbol": {
      "type": "string",
      "length": 7
    },
    "currency_name": {
      "type": "string",
      "length": 15
    },
    "phone": {
      "type": "string",
      "length": 7
    },
    "postal_code_format": {
      "type": "string",
      "length": 60
    },
    "languages": {
      "type": "string",
      "length": 100
    },
    "geonameid": {
      "type": "number"
    },
    "geo_region_id": {
      "type": "number"
    },
    "current_rate": {
      "type": "number",
      "dataType": "decimal",
      "precision": 6,
      "scale": 2
    }
  },
  "validations": [],
  "relations": {},
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
