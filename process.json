{
    "name": "sfbackend",
    "script": "./app.js",
    "instances": 1,
    "merge_logs": true,
    "log_date_format": "YYYY-MM-DD HH:mm Z",
    "watch": [
        "server",
        "common"
    ],
    "ignore_watch": [
        "node_modules",
        "client"
    ],
    "watch_options": {
        "followSymlinks": true,
        "usePolling": true,
        "interval": 5
    }
}
